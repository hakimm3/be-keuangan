    {
      "version": 2,
      "framework": null,
      "functions": {
        "api/index.php": {
          "runtime": "vercel-php@0.6.0"
        }
      },
      "routes": [{ "src": "/(.*)", "dest": "/api/index.php" }],
      "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourprojectdomain.com", // Replace with your domain
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp"
      }
    }